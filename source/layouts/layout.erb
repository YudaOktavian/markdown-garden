    <title>CommonMark</title>
    <%= stylesheet_link_tag "normalize", "all" %>
    <%= javascript_include_tag  "all" %>
  </head>
  <body>
    <div id="overall">
      <header>
        <nav>
          <ul>
            <li><%= link_to "Home", "/" %></li>
